{
  "name": "@dclexplorer/demo-scenes",
  "version": "1.0.0",
  "description": "Goerli Plaza tested scenes on alternative explorers",
  "scripts": {
    "build": "sdk-commands build --skip-install",
    "deploy": "sdk-commands deploy",
    "start": "sdk-commands start --skip-build -w",
    "export-static": "sdk-commands export-static --destination static/ --realmName demo-scenes --baseUrl https://dclexplorer.github.io/demo-scenes/ --commsAdapter ws-room:ws-room-service.decentraland.org/rooms/demo-scenes",
    "sync": "syncpack format --config .syncpackrc.json  --source \"*/package.json\" --source package.json",
    "test-sync": "syncpack list-mismatches --config .syncpackrc.json  --source \"*/package.json\" --source package.json"
  },
  "devDependencies": {
    "@dcl/js-runtime": "^7.4.7",
    "@dcl/sdk": "^7.4.7",
    "glob": "^10.3.10",
    "prettier": "3.0.3",
    "syncpack": "^11.2.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none"
  },
  "workspaces": [
    "basic-interactions",
    "boids",
    "cannon-car-example-scene",
    "coin-pickup",
    "cube-wave-16x16",
    "cube-wave-32x32",
    "dance-floor",
    "dbmonster",
    "droid-scene",
    "gnark",
    "hypno-wheels",
    "inflatable-punch-bag",
    "item-pickup",
    "laser-ray-casting",
    "media-streaming",
    "moving-platforms",
    "open-door",
    "party-time",
    "random-noise-movement",
    "swimming-shark",
    "translocator",
    "ui-canvas-information",
    "uv-map",
    "validate-player-authenticity"
  ],
  "files": [
    "static"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dclexplorer/demo-scenes.git"
  }
}
